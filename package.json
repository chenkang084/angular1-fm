{
  "name": "webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "presetup": "cnpm install",
    "setup": "npm run clean && npm run build-dll",
    "start": "cross-env NODE_ENV=local webpack-dev-server --content-base ./ --progress --colors --inline --hot --compress --history-api-fallback --port 8090",
    "run-qa": "cross-env NODE_ENV=qa webpack-dev-server --content-base public/ --progress --colors --inline --hot --compress --history-api-fallback --port 8090",
    "run-prod": "cross-env NODE_ENV=prod webpack-dev-server --content-base public/ --progress --colors --inline --hot --compress --history-api-fallback --port 8090",
    "build-local": "cross-env NODE_ENV=local webpack --content-base public/ --progress --colors --inline --compress",
    "build-qa": "npm run build-dll && cross-env NODE_ENV=qa webpack --content-base public/ --progress --colors --inline --compress",
    "build-prod": "npm run build-dll && cross-env NODE_ENV=prod webpack --content-base public/ --progress --colors --inline --compress",
    "clean": "rimraf ./app/assets/dll/* && rimraf ./dist/*",
    "build-dll": "npm run clean && webpack --colors --profile --display-modules --config webpack.dll.config.js ",
    "lint": "eslint --fix --ext .js app/core/ "
  },
  "author": "chenkang084@163.com",
  "license": "ISC",
