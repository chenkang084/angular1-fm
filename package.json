    "angular-sanitize": "1.5.6",
    "angular-ui-bootstrap": "~1.3.3",
    "animate.css": "^3.5.2",
    "autoprefixer": "^6.5.4",
    "better-npm-run": "0.0.13",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "css-loader": "^0.26.1",
    "es6-promise": "^4.0.5",
    "font-awesome": "^4.7.0",
    "html-loader": "^0.4.4",
    "jquery": "^3.1.1",
    "lodash": "^4.17.2",
    "moment": "^2.17.1",
    "ng-annotate-webpack-plugin": "^0.1.3",
    "node-sass": "^3.4.2",
    "nprogress": "^0.2.0",
    "postcss-loader": "^1.2.0",
    "resolve-url-loader": "^1.6.1",
    "restangular": "^1.6.1",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
